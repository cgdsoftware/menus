{
    "name": "laravel-enso/menus",
    "description": "Menu Manager dependency for Laravel Enso",
    "keywords": [
        "laravel-enso",
        "menus",
        "menu-manager",
        "breadcrumbs",
        "laravel-menu"
    ],
    "homepage": "https://github.com/laravel-enso/Menus",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://laravel-enso.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel-enso/core": "4.4.*",
        "laravel-enso/forms": "3.2.*",
        "laravel-enso/migrator": "1.0.*",
        "laravel-enso/permissions": "3.2.*",
        "laravel-enso/roles": "3.2.*",
        "laravel-enso/tables": "2.2.*"
    },
    "autoload": {
        "psr-4": {
            "LaravelEnso\\Menus\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelEnso\\Menus\\AppServiceProvider"
            ],
            "aliases": {}
        }
    }
}
